<div class="container">
  <div class = "row">
    <h2><%= @wiki.title %></h2>
    <p><%= @wiki.body %></p>
    <p><%= link_to "Edit", edit_wiki_path(@wiki) %> |
    <%= link_to "Delete", @wiki, method: :delete, data: { confirm: "Are you sure?" } %> |
    <%= link_to "Return to All Wikis", wikis_path %></p>
  </div>
</div>
